@{
    ViewBag.Title = "信息查询";
}
<div class="panel-body">
    <div class="panel panel-default">
        <div class="panel-heading">查询条件</div>
        <div class="panel-body">
            <form id="formSearch" class="form-horizontal">
                <div class="form-group">
                    <label class="control-label col-sm-1" for="txtBeginTime">开始时间</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control form_datetime" id="txtBeginTime">
                    </div>
                    <label class="control-label col-sm-1" for="txtEndTime">结束时间</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control form_datetime" id="txtEndTime">
                    </div>
                    <label class="control-label col-sm-1" for="txtCategory">类型</label>
                    <div class="col-sm-3">
                        <select class="form-control" id="txtCategory">
                            <option value="" selected>请选择</option>
                            <option value="002001006">工程建设</option>
                            <option value="002002003">政府采购</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-1" for="txtTitle">标题</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control" id="txtTitle">
                    </div>
                    <label class="control-label col-sm-1" for="txtSuccessfulName">中标企业</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control" id="txtSuccessfulName">
                    </div>
                    <div class="col-sm-4" style="text-align:left;">
                        <button type="button" style="margin-left:20px" id="btnQuery" class="btn btn-primary">查询</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <table id="tb_projectinfo"></table>
</div>
<!-- /.content -->
@section Css{
    <link href="~/PlugIns/WdatePicker/skin/WdatePicker.css?v=19" rel="stylesheet" />
}
@section scripts{
    <script src="/PlugIns/WdatePicker/WdatePicker.js?v=19"></script>
    <script src="~/Scripts/business/projectinfo.js?v=19"></script>
}


